#include <iostream>
using namespace std;

class Rectangle{
    public:
        Rectangle(int w = 1.0, int l = 1.0);
        void setWidth(int w);
        void setLength(int l);
        void input();
        double getWidth() const;
        double getLength() const;
        double area() const;
    private:
        double length;
        double width;
}

Rectangle()::Rectangle(int w, int l){
    width = w;
    length = l;
}

void Rectangle::setWidth(int w){
    width = w;
}

void Rectanlge::setLength(int l){
    length = 1;
}
